{
  "Environment": {
    "EnvironmentName": "Development",
    "IsDevelopment": true,
    "IsTest": true,
    "IsProduction": false,
    "IsDebug": true
  },
  "Api": {
    "ApiUrl": "https://dev-api.example.com",
    "Timeout": 30,
    "RetryCount": 1,
    "RetryDelay": 100,
    "EnableRequestLogging": true
  },
  "Ui": {
    "Browser": "Chrome",
    "Headless": false,
    "ImplicitWait": 10,
    "PageLoadTimeout": 30
  },
  "Logging": {
    "MinimumLevel": "Debug",
    "EnableConsoleLogging": true,
    "EnableFileLogging": true,
    "IncludeStructuredProperties": true
  }
} 